    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug TS with Params",
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "args": [
                "src/index.ts"
            ],
            "env": {
                "PRODUCERS_COUNT": "8",
                "MAX_NUMBER": "200000"
            },
            "cwd": "${workspaceFolder}",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
